{
    "name": "woost/laravel-prismic",
    "description": "Prismic provider for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Rutger Bresjer",
            "email": "rutger@woost.tech"
        }
    ],
    "require": {
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "prismic/php-sdk": "^5.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Woost\\LaravelPrismic\\ServiceProvider"
            ],
            "aliases": {
                "LaravelPrismic": "Woost\\LaravelPrismic\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Woost\\LaravelPrismic\\": "src/"
        }
    }
}
